{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "css.customData": [
    ".vscode/tailwind.json"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.wordWrap": "wordWrapColumn",
  "editor.columnSelection": false,
  "editor.wordWrapColumn": 80,
}